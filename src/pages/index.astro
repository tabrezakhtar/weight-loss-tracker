---
import Welcome from "../components/Welcome.astro";
import Layout from "../layouts/Layout.astro";
---

<Layout>
  <Welcome />
</Layout>

<script>
  // Add support for Material Dynamic Colors
  document.addEventListener("DOMContentLoaded", () => {
    if (window.materialDynamicColors) {
      window.materialDynamicColors.applyToPage();
    }
  });
</script>
